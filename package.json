{
  "name": "react-project",
  "description": "Fast and effective environment for frontend development",
  "author": "Maxim Lyulyaev",
  "version": "1.0.0",
  "scripts": {
    "server": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "---------- DEVELOPMENT --------": "",
    "predev": "rimraf dist && yarn babel",
    "dev": "tsc -watch -project configs/tsconfig.server.json & cross-env BABEL_ENV=client NODE_ENV=development nodemon --config configs/nodemon.json ./dist/server/index.js",
    "---------- PRODUCTION ---------": "",
    "prebuild": "rimraf dist && yarn babel",
    "build": "cross-env BABEL_ENV=client NODE_ENV=production webpack -p",
    "---------- HELPES -------------": "",
    "dockerbuild": "yarn version-increment && yarn prod:build && sudo docker build -t lyulyaevma/react-project:$(cat version.num) . && sudo docker push lyulyaevma/react-project:$(cat version.num)",
    "version-increment": "perl -i -pe 's/^((\\d+\\.)*)(\\d+)(.*)$/$1.($3+1).$4/e' < version.num version.num",
    "babel": "babel ./src --out-dir=dist --extensions .ts --extensions .tsx"
  },
